<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Description>A nhentai downloader.</Description>
    <Copyright>Â© Aiko Fujimoto. All Rights Reserved.</Copyright>
    <RepositoryUrl>https://github.com/aikoofujimotoo/asuka</RepositoryUrl>
    <ApplicationIcon>Resources\appicon.ico</ApplicationIcon>
    <Version>0.1.4</Version>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <FileVersion>0.1.4.0</FileVersion>
    <AssemblyVersion>0.1.4.0</AssemblyVersion>
    <PackageIcon>AppIcon.png</PackageIcon>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove=".circleci\**" />
    <Compile Remove="docs\**" />
    <EmbeddedResource Remove=".circleci\**" />
    <EmbeddedResource Remove="docs\**" />
    <None Remove=".circleci\**" />
    <None Remove="docs\**" />
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="Resources\AppIcon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.8.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="RestSharp" Version="106.11.7" />
    <PackageReference Include="Sharprompt" Version="2.0.0" />
    <PackageReference Include="ShellProgressBar" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
